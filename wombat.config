%% Ignored alarms by default. Add ids to disable or remove ids to enable alarms.
{set, wo_alarms, ignored_alarm_ids, [old_code, module_clash, missing_modules]}.
%% remove the following line to raise alarms when different module versions exist in the cluster
{set, wo_alarms, ignore_different_module_versions, ".*"}.
%% remove the following line to raise alarms when different application versions exist in the cluster
{set, wo_alarms, ignore_different_application_versions, ".*"}.
%% remove the following line to raise alarms when an application is missing in the cluster
{set, wo_alarms, ignore_missing_application, ".*"}.


{set, wo_plugins, plugins, etop_txt, [{enable, true},
                                      {max_size, 1000},
                                      {procs, 200},
                                      {period, 2},
                                      {sort_by, msg_queue}]}.

{set,wo_int,prometheus,[{enabled,true}]}.

